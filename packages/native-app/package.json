{
  "name": "native-app",
  "version": "0.1.0",
  "private": true,
  "author": "Christoph Benjamin Weber",
  "license": "MIT",
  "scripts": {
    "prepublish": "rimraf ./universal-components && ncp ../universal-components/dist ./universal-components",
    "prestart": "rimraf ./universal-components && ncp ../universal-components/dist ./universal-components",
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "test": "jest",
    "lint": "eslint ./App*.js src"
  },
  "dependencies": {
    "expo": "^27.0.1",
    "react": "16.3.1",
    "react-native": "~0.55.2",
    "react-native-calendars": "^1.19.3",
    "react-native-elements": "^0.19.1",
    "react-native-sideswipe": "^1.4.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.4.0",
    "jest-expo": "~27.0.0",
    "ncp": "^2.0.0",
    "prettier": "^1.13.5",
    "react-native-scripts": "1.14.0",
    "react-test-renderer": "16.3.1",
    "rimraf": "^2.6.2"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "jest": {
    "preset": "jest-expo"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "import/no-extraneous-dependencies": false,
      "react/prop-types": "warning",
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "no-unused-vars": [
        "error",
        {
          "vars": "all",
          "args": "after-used",
          "argsIgnorePattern": "^_",
          "ignoreRestSiblings": true
        }
      ],
      "react/prefer-stateless-function": false
    },
    "extends": [
      "airbnb",
      "prettier",
      "prettier/react"
    ]
  }
}
